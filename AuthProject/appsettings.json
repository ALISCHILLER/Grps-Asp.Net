{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  {
    "ConnectionStrings": {
      "DefaultConnection": "Server=your_server;Database=AuthDb;User Id=your_user;Password=your_password;"
    },
    "Jwt": {
      "Key": "your_super_secret_key_32_bytes_long",
      "Issuer": "https://localhost:5001",
      "Audience": "https://localhost:5001",
      "ExpiryInMinutes": 30
    }
  }
}
